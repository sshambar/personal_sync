<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN"
                "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version='1.0'>
<dict>
	<key>Label</key>
	<string>dhcpd-watch</string>
	<key>ProgramArguments</key>
	<array>
	  <string>/usr/local/startup/dhcpd-watch</string>
	  <string>vboxnet0</string>
	</array>
	<key>StandardErrorPath</key>
	<string>/usr/local/var/log/dhcpd-watch.log</string>
	<key>ProcessType</key>
	<string>Background</string>
	<key>RunAtLoad</key>
	<true/>
	<key>ExitTimeOut</key>
	<integer>5</integer>
</dict>
</plist>
